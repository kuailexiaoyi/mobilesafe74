<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <TextView 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="设置中心"
        style="@style/TitleTheme"/>
    <com.zrblog.mobilesafe74.view.SettingItemView 
        xmlns:mobilesafe="http://schemas.android.com/apk/res/com.zrblog.mobilesafe74"
        android:id="@+id/set_item_update"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
       	mobilesafe:destitle = "自动更新设置"
       	mobilesafe:deson = "自动更新开启"
       	mobilesafe:desoff = "自动更新关闭"
        />
    <com.zrblog.mobilesafe74.view.SettingItemView 
        xmlns:mobilesafe="http://schemas.android.com/apk/res/com.zrblog.mobilesafe74"
        android:id="@+id/set_item_phone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
       	mobilesafe:destitle = "电话归属地更新设置"
       	mobilesafe:deson = "归属地显示已开启"
       	mobilesafe:desoff = "归属地显示已关闭"
        />

</LinearLayout>
